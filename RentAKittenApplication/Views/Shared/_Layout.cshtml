<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <!--  <title>@ViewBag.Title - My ASP.NET Application</title>  -->
    <title>Rent-A-Kitten</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <!-- custom stylesheet and favicon -->
    <link rel="shortcut icon" type="image/ico" href="~/Content/favicon.ico">
    <link href="~/Content/StyleSheet.css" rel="stylesheet" />
</head>
<body>
    <ul class="nav nav-pills">
        <li role="presentation" class="active">@Html.ActionLink("Home", "Index", "Home")</li>
        <li role="presentation">@Html.ActionLink("Kittens", "../Kittens")</li>
     </ul>

 
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year | Rent-A-Kitten</p>
        </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    

</body>
</html>
